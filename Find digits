#include <bits/stdc++.h>

using namespace std;

int main(){
    int t;
    cin>>t;
    for(int i=0;i<t;i++){
        int n;
        cin>>n;

        int temp=n,r,cnt=0;
        while(temp){
            r=temp%10;
            temp=temp/10;
            if(r !=0 && n%r==0){
                 cnt++;
            }        
        }
        cout<<cnt<<endl;
    }
    return 0;
}    
